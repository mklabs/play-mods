# This file defines all module routes (Higher priority routes first)
#
# import these routes in the main app as :
# *     /                       module:html5boilerplate
#
# ~~~~

# Map the static resources from the /support/html5-boilerplate-v0.9.5 folder to the /boilerplate path
GET        /boilerplate/        staticDir:support/html5-boilerplate-v0.9.5